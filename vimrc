syntax on
set background=dark
set ruler
se t_Co=16
let g:solarized_termcolors=256
set wildmode=list:longest
colorscheme solarized 
set tabstop=4
set autoindent
set copyindent
set smartindent
set confirm
set showmatch
set wildmenu
set wildmode=longest:full,full
set number
set backspace=2
set shiftwidth=4
set hlsearch
set title
set incsearch

call pathogen#infect()
filetype plugin indent on
